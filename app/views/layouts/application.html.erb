<!DOCTYPE html>
<html>

<head>
  <title>Club Emprendedores</title>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <%= csrf_meta_tags %>

  <%= stylesheet_pack_tag 'stylesheets/application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'javascript/application', 'data-turbolinks-track': 'reload' %>

  <%= component 'pwa/manifest', url: request.base_url %>
</head>

<body>

  <div id="body-wrapper">

    <%= render 'shared/menu' %>

    <div id="main-body">
      <!-- <div class="encabezado"> -->
      <%= render 'shared/user_status' %>

      <!-- Notificaciones de Devise -->
      <% if !notice.nil? %>
      <p class="alert alert-success">
        <%= notice %>
      </p>
      <% end %>
      <% if !alert.nil? %>
      <p class="alert alert-danger">
        <%= alert %>
      </p>
      <% end %>

      <%= yield %>
    </div>
    <div id="push"></div>
  </div>
  <%= render 'shared/footer' %>
</body>

</html>